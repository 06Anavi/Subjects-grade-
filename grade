#include <iostream>
using namespace std;
int main() {
    int mark1, mark2, mark3;
    float average;
    cout << "Enter 3 subject marks: ";
    cin >> mark1 >> mark2 >> mark3;
    average = (mark1 + mark2 + mark3) / 3.0;
    cout << "Average: " << average << endl;
    if (average >= 90)
        cout << "Grade: A" << endl;
    else if (average >= 80)
        cout << "Grade: B" << endl;
    else if (average >= 70)
        cout << "Grade: C" << endl;
    else if (average >= 60)
        cout << "Grade: D" << endl;
    else
        cout << "Grade: F" << endl;
    return 0;
}
